{
  "include-v-in-tag": true,
  "separate-pull-requests": true,
  "packages": {
    ".github/workflows/golang-ci.yml": {
      "release-type": "simple",
      "package-name": "golang-ci",
      "changelog-path": "CHANGELOGS/golang-CHANGELOG.md"
    },
    ".github/workflows/golang-release.yml": {
      "release-type": "simple",
      "package-name": "golang-release",
      "changelog-path": "CHANGELOGS/golang-CHANGELOG.md"
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
